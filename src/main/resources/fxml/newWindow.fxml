<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import com.jfoenix.validation.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controllers.OpenWindow">
   <children>
      <Pane fx:id="workPane" prefHeight="401.0" prefWidth="602.0">
         <children>
            <ComboBox fx:id="files" layoutX="17.0" layoutY="24.0" onAction="#addFiles" prefHeight="31.0" prefWidth="157.0" />
            <ListView id="viewFile" fx:id="viewFile" layoutX="17.0" layoutY="97.0" onMouseClicked="#choiceFiles" prefHeight="268.0" prefWidth="157.0" />
            <ListView fx:id="choiceFile" layoutX="229.0" layoutY="97.0" prefHeight="268.0" prefWidth="157.0" />
            <Button fx:id="openFile" layoutX="428.0" layoutY="24.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="157.0" text="Button" />
            <TextArea fx:id="textFromFile" layoutX="428.0" layoutY="81.0" prefHeight="132.0" prefWidth="157.0" />
            <LineChart fx:id="chart" layoutX="396.0" layoutY="231.0" prefHeight="150.0" prefWidth="190.0">
              <xAxis>
                <CategoryAxis side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis side="LEFT" />
              </yAxis>
            </LineChart>
            <Button fx:id="back" layoutX="184.0" layoutY="193.0" mnemonicParsing="false" onAction="#removeFiles" prefHeight="23.0" prefWidth="36.0" text="Button" />
            <Button fx:id="ahead" layoutX="184.0" layoutY="231.0" mnemonicParsing="false" onAction="#moveFiles" prefHeight="31.0" prefWidth="36.0" text="Button" />
            <Button fx:id="deleteFile" layoutX="138.0" layoutY="366.0" mnemonicParsing="false" onAction="#deleteFiles" prefHeight="31.0" prefWidth="36.0" text="Button" />
         </children>
      </Pane>
   </children>
</AnchorPane>
